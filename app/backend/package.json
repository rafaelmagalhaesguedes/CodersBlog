{
  "name": "authts",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "tsnd --watch \"./src/**\" ./src/server.ts",
    "predev": "npm run db:reset",
    "test": "mocha -r ts-node/register tests/**/**/*.test.ts --exit -t 60000 --exit",
    "test:coverage": "nyc mocha -r ts-node/register tests/**/**/*.test.ts --exit -t 60000",
    "lint": "tsc && eslint . --ext .ts",
    "compose:up": "docker-compose up -d --build",
    "compose:down": "docker-compose down --remove-orphans",
    "db:reset": "npx -y tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "keywords": [],
  "author": "Rafael Magalh√£es Guedes",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "9.1.0",
    "@types/sequelize": "^4.28.20",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "^8.57.0",
    "mocha": "9.2.1",
    "nyc": "15.1.0",
    "sequelize-cli": "^6.6.2",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
